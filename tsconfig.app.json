{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.json",
    "src/**/*.ts",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "types": [
      "naive-ui/volar"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
  }
}